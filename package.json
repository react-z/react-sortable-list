{
  "name": "react-sortable-list",
  "version": "1.0.0",
  "description": "a sortable list component using html5 drag and drop api",
  "main": "lib/sortable-list.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/StevenIseki/react-sortable-list.git"
  },
  "author": "Steven Iseki <stevenisekimartin@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": "^0.14.7"
  },
  "devDependencies": {
    "express": "^4.7.2",
    "react": "^0.14.7",
    "babel": "^5.6.7",
    "babel-core": "^5.6.7",
    "babel-loader": "^5.1.4",
    "node-libs-browser": "^0.5.2",
